<!DOCTYPE html>
<html>
<head>
  <title>Restaurants</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<div class="container">
	<div class="well">
		<div class="row">
			this is a test col.
		</div>
	</div>
	<% if flash[:notice].present? %>
	<p><%= flash[:notice] %></p>
	<% end%>
	<% if flash[:alert].present? %>
	<p><%= flash[:alert] %></p>
	<% end %>
	<%= yield %>
</div>
</body>
</html>
